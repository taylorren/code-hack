{% set active=4 %}
{% extends 'layout.html.twig' %}

{% block title %}
    源代码 | 跟我一起学Hack
{% endblock %}

{% block content %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">{{file}}源代码</h3>
        </div>
        <div class="panel-body">
            <pre class="bg-black green">{{src}}</pre>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">{{file}}输出</h3>
        </div>
        <div class="panel-body">
            <pre>{{out}}</pre>
        </div>
    </div>

{% endblock %}